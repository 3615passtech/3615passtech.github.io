{
  "name": "3615passtech.github.io",
  "version": "0.1.0",
  "private": true,
  "description": "Développez vos compétences numériques en trouvant la meilleure formation !",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3615passtech/3615passtech.github.io.git"
  },
  "author": "Nicolas Hovart <hovart.nicolas@gmail.com>, Yvan Sraka <yvan@sraka.pw>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/3615passtech/3615passtech.github.io/issues"
  },
  "homepage": "https://3615passtech.github.io",
  "dependencies": {
    "@material/button": "^0.36.0",
    "@material/layout-grid": "^0.41.0",
    "@material/textfield": "^0.36.0",
    "gh-pages": "^2.0.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
